@using Microsoft.AspNetCore.Identity;
@model IEnumerable<IdentityUser>


<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Username</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model)
    {
      <tr>
        <th scope="row">@item.Id</th>
        <td>@item.UserName</td>
      </tr>
    }
  </tbody>
</table>